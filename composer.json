{
    "name": "calliostro/spotify-web-api-bundle",
    "description": "Spotify Web API client for Symfony 5 or Symfony 6",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.3 || ^8.0",
        "jwilsson/spotify-web-api-php": "^5.0",
        "symfony/config": "^5.0 || ^6.0",
        "symfony/dependency-injection": "^5.0 || ^6.0",
        "symfony/http-kernel": "^5.0 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Calliostro\\SpotifyWebApiBundle\\": "src/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0 || ^6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Calliostro\\SpotifyWebApiBundle\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "calliostro",
            "email": "calliostro@gmail.com"
        }
    ]
}
