{
    "name": "calliostro/spotify-web-api-bundle",
    "description": "Spotify Web API client for Symfony 6.4 and Symfony 7",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.1",
        "jwilsson/spotify-web-api-php": "^6.0",
        "symfony/config": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "symfony/http-kernel": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Calliostro\\SpotifyWebApiBundle\\": "src/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.4|^7.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Calliostro\\SpotifyWebApiBundle\\Tests\\": "tests/"
        }
    },
    "scripts":{
        "test": "phpunit tests"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "calliostro",
            "email": "calliostro@gmail.com"
        }
    ]
}
